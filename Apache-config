<VirtualHost *:443>
    ServerName ims-kld-app.infinityfreeapp.com

    SSLEngine on
    SSLCertificateFile /etc/letsencrypt/live/ims-kld-app.infinityfreeapp.com/fullchain.pem
    SSLCertificateKeyFile /etc/letsencrypt/live/ims-kld-app.infinityfreeapp.com/privkey.pem

    DocumentRoot /var/www/myapp/frontend/dist

    # Proxy WebSocket
    ProxyPass "/ws"  "ws://127.0.0.1:8080/"
    ProxyPassReverse "/ws"  "ws://127.0.0.1:8080/"
</VirtualHost>